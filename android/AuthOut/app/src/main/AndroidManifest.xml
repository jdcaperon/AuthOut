<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rocketpotatoes.authout">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/white_tick"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/authout_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Launcher">
        <activity
            android:name=".HomeActivity"
            android:theme="@style/AppTheme.Launcher">
        </activity>
        <activity android:name=".SelectStudentActivity" android:theme="@style/AppTheme" />
        <activity android:name=".ConfirmationActivity" android:theme="@style/AppTheme" />
        <activity android:name=".EnterCodeActivity" android:theme="@style/AppTheme" />
        <activity android:name=".AdminActivity" android:theme="@style/AppTheme" />
        <activity android:name=".SignUpActivity" android:theme="@style/AppTheme">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </application>

</manifest>